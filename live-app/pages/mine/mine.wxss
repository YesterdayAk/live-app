}
.mine .contentBox .content .content-top .information .information-center .finish .title {
  font-size: 26rpx;
  font-weight: normal;
  color: #333333;
  line-height: 26rpx;
  margin: 20rpx 0 0;
}
.mine .contentBox .content .content-top .information .information-center .start {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin: 0 54rpx 0 0;
}
.mine .contentBox .content .content-top .information .information-center .start .start_sign {
  width: 60rpx;
  height: 60rpx;
  background: url("https://res1.qdszgh.cn/%E9%B8%BF%E9%9B%81%E8%9E%8D%E7%9B%B4%E6%92%AD/start_sign.png");
  background-size: 100% 100%;
  position: relative;
}
.mine .contentBox .content .content-top .information .information-center .start .title {
  font-size: 26rpx;
  font-weight: normal;
  color: #333333;
  line-height: 26rpx;
  margin: 20rpx 0 0;
}
.mine .contentBox .content .content-center {
  width: 100%;
  display: flex;
  flex-direction: column;
  margin: 32rpx 0 0;
}
.mine .van-cell {
  display: flex;
  align-items: center;
  height: 96rpx;
}
