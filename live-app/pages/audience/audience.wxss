.audience {
  display: flex;
  flex-direction: column;
  min-height: 100vh;
  background-size: 100% 100%;
  background-repeat: no-repeat;
  background-image: url('https://res1.qdszgh.cn/%E9%B8%BF%E9%9B%81%E8%9E%8D%E7%9B%B4%E6%92%AD/live_bg.png');
}
.audience .introduceBox {
  width: 100%;
  display: flex;
  position: absolute;
  top: 0;
}
.audience .introduceBox .introduce {
  display: flex;
  flex-direction: column;
  margin: 0 0 0 20rpx;
}
.audience .introduceBox .introduce .introduce-top {
  display: flex;
}
.audience .introduceBox .introduce .introduce-top .stream {
  display: flex;
  margin: 14rpx 0 16rpx;
  background: rgba(0, 0, 0, 0.26);
  border-radius: 82rpx;
}
.audience .introduceBox .introduce .introduce-top .stream .stream-status {
  width: 120rpx;
  height: 52rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background: rgba(0, 0, 0, 0.26);
  border-radius: 0 82rpx 82rpx 0;
}
.audience .introduceBox .introduce .introduce-top .stream .stream-status .message {
  font-size: 22rpx;
  font-weight: normal;
  color: #fff;
  margin: 0 0 0 4rpx;
  line-height: 32rpx;
}
.audience .introduceBox .introduce .introduce-top .stream .stream-theme {
  width: 376rpx;
  display: flex;
  align-items: center;
}
.audience .introduceBox .introduce .introduce-top .stream .stream-theme .title {
  font-size: 28rpx;
  font-weight: normal;
  color: #fff;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  line-height: 32rpx;
  margin: 0 26rpx 0 14rpx;
}
.audience .introduceBox .introduce .introduce-center {
  width: 340rpx;
  height: 48rpx;
  border-radius: 236rpx;
  display: flex;
  align-items: center;
  background: rgba(0, 0, 0, 0.26);
}
.audience .introduceBox .introduce .introduce-center .introduce-center-left {
  display: flex;
}
.audience .introduceBox .introduce .introduce-center .introduce-center-left .brief_normal {
  display: flex;
}
.audience .introduceBox .introduce .introduce-center .introduce-center-left .brief_normal .message {
  font-size: 22rpx;
  font-weight: normal;
  color: #fff;
  line-height: 22rpx;
  margin: 0 6rpx 0 16rpx;
}
.audience .introduceBox .introduce .introduce-center .introduce-center-left .brief_active {
  display: flex;
}
.audience .introduceBox .introduce .introduce-center .introduce-center-left .brief_active .message {
  font-size: 22rpx;
  font-weight: normal;
  color: #ff2a00;
  line-height: 22rpx;
  margin: 0 6rpx 0 16rpx;
}
.audience .introduceBox .introduce .introduce-center .introduce-center-right {
  display: flex;
  margin: 0 0 0 30rpx;
}
.audience .introduceBox .introduce .introduce-center .introduce-center-right .browse {
  display: flex;
}
.audience .introduceBox .introduce .introduce-center .introduce-center-right .browse .number {
  font-size: 22rpx;
  font-weight: normal;
  color: #FFFFFF;
  line-height: 26rpx;
  margin: 0 0 0 6rpx;
}
.audience .introduceBox .introduce .introduce-center .introduce-center-right .like {
  display: flex;
  margin: 0 0 0 10rpx;
}
.audience .introduceBox .introduce .introduce-center .introduce-center-right .like .number {
  font-size: 22rpx;
  font-weight: normal;
  color: #FFFFFF;
  line-height: 26rpx;
  margin: 0 0 0 6rpx;
}
.audience .introduceBox .introduce .introduce-bottom {
  margin: 14rpx 0 0;
}
.audience .introduceBox .introduce .introduce-bottom image {
  width: 116rpx;
  height: 48rpx;
}
.audience .review {
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  position: absolute;
  bottom: 98rpx;
}
.audience .review.active {
  bottom: 166rpx;
}
.audience .review .information {
  width: 95%;
  display: flex;
  flex-direction: column;
  margin: 0 0 32rpx 0;
}
.audience .review .information .container {
  width: 390rpx;
  display: flex;
  height: 88rpx;
  background: rgba(255, 42, 0, 0.34);
  border-radius: 460rpx;
}
.audience .review .information .container .info {
  display: flex;
  flex-direction: column;
}
.audience .review .information .container .info .name {
  width: 180rpx;
  font-size: 30rpx;
  font-weight: normal;
  color: #ffffff;
  line-height: 26rpx;
  margin: 16rpx 0 8rpx 8rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.audience .review .information .container .info .message {
  font-size: 24rpx;
  font-weight: normal;
  color: #ffffff;
  line-height: 26rpx;
  margin: 0 0 0 10rpx;
}
.audience .review .information .container .gift {
  display: flex;
  align-items: flex-end;
}
.audience .review .information .container .gift image {
  width: 136rpx;
  height: 136rpx;
}
.audience .review .comment {
  width: 100%;
  display: flex;
  justify-content: center;
}
.audience .review .comment .comment_scroll {
  width: 100%;
  display: flex;
  margin: 10rpx 0 0;
}
.audience .review .comment .comment_scroll .comment_list {
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.audience .review .comment .comment_scroll .comment_list .comment_item_container {
  width: 95%;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.audience .review .comment .comment_scroll .comment_list .comment_item_container .container_top {
  width: 100%;
  display: flex;
  margin: 0 0 20rpx 0;
}
.audience .review .comment .comment_scroll .comment_list .comment_item_container .container_top .comment_item {
  display: flex;
  background: rgba(0, 0, 0, 0.3);
  border-radius: 12rpx;
  padding: 10rpx;
  word-break: break-all;
}
.audience .review .comment .comment_scroll .comment_list .comment_item_container .container_top .comment_item .comment_item_left {
  display: flex;
}
.audience .review .comment .comment_scroll .comment_list .comment_item_container .container_top .comment_item .comment_item_left image {
  width: 52rpx;
  height: 52rpx;
  border-radius: 50%;
  vertical-align: middle;
}
.audience .review .comment .comment_scroll .comment_list .comment_item_container .container_top .comment_item .comment_item_right {
  display: flex;
  flex-direction: column;
  margin: 0 0 0 10rpx;
}
.audience .review .comment .comment_scroll .comment_list .comment_item_container .container_top .comment_item .comment_item_right .name {
  font-size: 30rpx;
  font-weight: normal;
  color: #ffffff;
  vertical-align: middle;
}
.audience .review .comment .comment_scroll .comment_list .comment_item_container .container_top .comment_item .comment_item_right .message {
  font-size: 28rpx;
  font-weight: normal;
  color: #FFFFFF;
  vertical-align: middle;
}
.audience .review .comment .comment_scroll .comment_list .comment_item_container .container_center {
  width: 100%;
  display: flex;
  justify-content: center;
}
.audience .review .comment .comment_scroll .comment_list .comment_item_container .container_center .gift {
  display: flex;
  background: rgba(0, 0, 0, 0.3);
  border-radius: 24rpx;
  padding: 10rpx 40rpx;
}
.audience .review .comment .comment_scroll .comment_list .comment_item_container .container_center .gift .name,
.audience .review .comment .comment_scroll .comment_list .comment_item_container .container_center .gift .message {
  font-size: 24rpx;
  font-weight: normal;
  color: #fff;
  line-height: 24rpx;
}
.audience .review .comment .comment_scroll .comment_list .comment_item_container .container_center .gift .give {
  font-size: 24rpx;
  font-weight: normal;
  color: #fff;
  line-height: 24rpx;
  margin: 0 10rpx;
}
.audience .review .comment .comment_scroll ::-webkit-scrollbar {
  display: none;
}
.audience .review .heart {
  position: absolute;
  right: 0;
  bottom: 0;
}
.audience .review .heart .live_like {
  width: 80rpx;
  height: 80rpx;
  position: absolute;
  bottom: 30rpx;
  right: 20rpx;
  background-size: 100% 100%;
  background-repeat: no-repeat;
  background-image: url('https://res1.qdszgh.cn/%E9%B8%BF%E9%9B%81%E8%9E%8D%E7%9B%B4%E6%92%AD/heart.png');
}
.audience .review .heart .live_like .count {
  display: inline-block;
  font-size: 26rpx;
  color: #fff;
  background: linear-gradient(90deg, #fa4454, #eb1d5a);
  position: absolute;
  padding: 0 8rpx;
  border-radius: 30rpx;
  left: 50%;
  /* 定位父级的50% */
  transform: translateX(-50%);
  /*自己的50% */
}
.audience .review .goodsBox {
  width: 100%;
  display: flex;
  justify-content: center;
  position: absolute;
  bottom: 0;
}
.audience .review .goodsBox .goods {
  width: 95%;
  display: flex;
  background: #FEFFFE;
  border-radius: 16rpx;
}
.audience .review .goodsBox .goods .picture {
  width: 172rpx;
  height: 172rpx;
  min-width: 172rpx;
  margin: 16rpx;
}
.audience .review .goodsBox .goods .information {
  display: flex;
  flex-direction: column;
  margin: 16rpx 20rpx 16rpx 0;
}
.audience .review .goodsBox .goods .information .title {
  font-size: 32rpx;
  font-weight: normal;
  color: #323332;
  line-height: 46rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}
.audience .review .goodsBox .goods .information .info {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin: 34rpx 0 0;
}
.audience .review .goodsBox .goods .information .info .info-left {
  display: flex;
  align-items: center;
}
.audience .review .goodsBox .goods .information .info .info-left .symbol {
  font-size: 28rpx;
  font-weight: 600;
  color: #FF2A00;
  line-height: 44rpx;
}
.audience .review .goodsBox .goods .information .info .info-left .price {
  font-size: 36rpx;
  font-weight: 600;
  color: #FF2A00;
  line-height: 44rpx;
}
.audience .review .goodsBox .goods .information .info .info-left .sell {
  font-size: 24rpx;
  font-weight: normal;
  color: #999999;
  line-height: 24rpx;
  margin: 0 0 0 14rpx;
}
.audience .review .goodsBox .goods .information .info .info-right {
  display: flex;
}
.audience .review .goodsBox .goods .information .info .info-right .buy {
  width: 122rpx;
  height: 42rpx;
  background: linear-gradient(270deg, #FE0D38 0%, #FC811F 100%);
  border-radius: 40rpx;
  font-size: 26rpx;
  font-weight: normal;
  color: #FEFFFE;
  line-height: 26rpx;
}
.audience .reply_wrp {
  width: 100%;
  position: fixed;
  bottom: 0;
}
.audience .reply_wrp .send {
  width: 100%;
  min-height: 100rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.audience .reply_wrp .send.show {
  margin: 0 0 30rpx 0;
}
.audience .reply_wrp .send .information {
  width: 95%;
  display: flex;
  align-items: center;
}
.audience .reply_wrp .send .information .mike {
  width: 80rpx;
  height: 80rpx;
  background: rgba(0, 0, 0, 0.3);
  border-radius: 122rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  margin: 0 18rpx 0 20rpx;
}
.audience .reply_wrp .send .information .mike image {
  width: 52rpx;
  height: 52rpx;
}
.audience .reply_wrp .send .information .shopping {
  display: flex;
  margin: 0 18rpx 0 20rpx;
}
.audience .reply_wrp .send .information .shopping image {
  width: 52rpx;
  height: 52rpx;
}
.audience .reply_wrp .send .information .info {
  width: 518rpx;
  height: 80rpx;
  background: rgba(0, 0, 0, 0.3);
  border-radius: 340rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.audience .reply_wrp .send .information .info .text {
  display: inline-block;
  width: 80%;
  height: 60rpx;
  margin: 0 0 0 28rpx;
  border: none;
  font-size: 28rpx;
  font-weight: normal;
  line-height: 28rpx;
  color: #fff;
}
.audience .reply_wrp .send .information .info image {
  width: 36rpx;
  height: 36rpx;
  margin: 0 28rpx 0 0;
}
.audience .reply_wrp .send .information .add {
  display: flex;
  margin: 0 0 0 20rpx;
}
.audience .reply_wrp .send .information .gift {
  width: 80rpx;
  height: 80rpx;
  background: rgba(0, 0, 0, 0.3);
  border-radius: 122rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  margin: 0 12rpx 0 20rpx;
}
.audience .reply_wrp .send .information .gift image {
  width: 52rpx;
  height: 52rpx;
}
.audience .reply_wrp .send .information .more {
  width: 80rpx;
  height: 80rpx;
  background: rgba(0, 0, 0, 0.3);
  border-radius: 122rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.audience .reply_wrp .send .information .more image {
  width: 52rpx;
  height: 52rpx;
}
.audience .reply_wrp .reply_panel_wrp {
  overflow: hidden;
  position: relative;
}
.audience .reply_wrp .reply_panel_wrp .reply_panel {
  overflow: auto;
  position: absolute;
  left: 0;
  right: 0;
  top: 600rpx;
  -webkit-transition: top .2s;
  transition: top .2s;
}
.audience .reply_wrp .reply_panel_wrp .reply_panel[hidden] {
  display: block;
}
.audience .reply_wrp .reply_panel_wrp .reply_panel.show {
  height: 600rpx;
  top: 0;
}
.audience .reply_wrp .reply_panel_wrp .reply_panel:before {
  content: "";
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  height: 2rpx;
  border-top: 1 rpx solid rgba(0, 0, 0, 0.1);
  color: rgba(0, 0, 0, 0.1);
}
.restBox {
  display: flex;
  justify-content: center;
}
.restBox .rest {
  width: 100%;
  display: flex;
  flex-direction: column;
}
.restBox .rest .rest-top {
  display: flex;
  justify-content: flex-end;
  background: #fff;
}
.restBox .rest .rest-top .close {
  width: 56rpx;
  height: 42rpx;
  font-size: 28rpx;
  font-weight: normal;
  color: #E45E5E;
  line-height: 42rpx;
  margin: 26rpx 40rpx 26rpx 0;
}
.restBox .rest .rest-center {
  display: flex;
  flex-direction: column;
  background: #f6f6f6;
  padding: 0 0 56rpx;
}
.restBox .rest .rest-center .live {
  display: flex;
  flex-direction: column;
  margin: 46rpx 0 0 40rpx;
}
.restBox .rest .rest-center .live .live-top {
  display: flex;
}
.restBox .rest .rest-center .live .live-top .title {
  font-size: 28rpx;
  font-weight: normal;
  color: #007AFF;
  line-height: 20rpx;
}
.restBox .rest .rest-center .live .live-bottom {
  display: flex;
}
.restBox .rest .rest-center .live .live-bottom .microphone {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin: 20rpx 0 0;
}
.restBox .rest .rest-center .live .live-bottom .microphone .microphone-top {
  width: 100rpx;
  height: 100rpx;
  position: relative;
  background-size: 100% 100%;
  background-repeat: no-repeat;
  background-image: url('https://res1.qdszgh.cn/%E9%B8%BF%E9%9B%81%E8%9E%8D%E7%9B%B4%E6%92%AD/microphone.png');
}
.restBox .rest .rest-center .live .live-bottom .microphone .microphone-top .count {
  display: inline-block;
  font-size: 20rpx;
  color: #fff;
  background: linear-gradient(90deg, #fa4454, #eb1d5a);
  position: absolute;
  padding: 0 8rpx;
  border-radius: 30rpx;
  left: 50%;
  bottom: 0;
  /* 定位父级的50% */
  transform: translateX(-50%);
  /*自己的50% */
}
.restBox .rest .rest-center .live .live-bottom .microphone .microphone-bottom .title {
  font-size: 24rpx;
  font-weight: normal;
  color: #333333;
  line-height: 24rpx;
}
